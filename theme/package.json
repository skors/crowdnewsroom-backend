{
  "name": "yet-another-webpack-es6-starterkit",
  "version": "1.0.0",
  "description": "A light foundation for your next frontend project based on webpack.",
  "scripts": {
    "build": "rm -rf static/js/*; rm -rf static/css/*; NODE_ENV=production webpack --config webpack.config.js --colors",
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.js --watch --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wbkd/yet-another-webpack-es6-starterkit.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "webkid.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wbkd/yet-another-webpack-es6-starterkit/issues"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "browserslist": [
    "last 2 versions"
  ],
  "homepage": "https://github.com/wbkd/yet-another-webpack-es6-starterkit#readme",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.26.1",
    "dom-classlist": "^1.0.1",
    "eslint": "^3.12.1",
    "eslint-loader": "^1.3.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.0.0",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.13.1",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^0.6.2",
    "webpack": "^4.16.0",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-webfonts": "^1.0.6",
    "babel-polyfill": "^6.9.0",
    "carbon-components": "^9.1.10",
    "carbon-components-react": "^6.7.4",
    "carbon-icons": "^7.0.7",
    "expose-loader": "^0.7.5",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "js-cookie": "^2.2.0",
    "moment": "^2.22.1",
    "query-string": "5",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "source-sans-pro": "^2.20.2"
  }
}
